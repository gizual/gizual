{
  "name": "e2e",
  "devDependencies": {
    "@playwright/test": "1.42.1",
    "express": "4.18.2",
    "http-proxy-middleware": "2.0.6",
    "vite-node": "1.0.2"
  },
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc",
    "e2e:local": "yarn e2e --ignore-snapshots",
    "e2e:update": "yarn e2e -u --retries 0",
    "e2e": "playwright test",
    "start-app": "vite-node ./local-preview.ts"
  },
  "packageManager": "yarn@3.5.0"
}
